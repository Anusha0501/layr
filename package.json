{
  "name": "layr",
  "displayName": "Layr",
  "description": "A simplified planning layer that helps developers think before coding by generating structured project plans",
  "version": "0.1.0",
  "publisher": "layr-dev",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "planning",
    "project",
    "structure",
    "development",
    "ai-inspired"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "layr.createPlan",
        "title": "Layr: Create Plan",
        "category": "Layr"
      },
      {
        "command": "layr.executePlan",
        "title": "Layr: Execute Plan",
        "category": "Layr"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/layr.git"
  },
  "license": "MIT"
}